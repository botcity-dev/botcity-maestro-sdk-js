    "license": "MIT",
    "devDependencies": {
        "@types/node": "^14.18.26",
        "news-fragments": "^2.0.5",
        "release-it": "^15.4.1",
        "rimraf": "^3.0.2",
        "typescript": "^4.2.4"
    },
    "dependencies": {
        "axios": "^0.27.2",
